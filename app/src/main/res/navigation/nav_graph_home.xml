<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.demo.jetpack.navigation.fragment.NavHomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_nav_home" />
    <fragment
        android:id="@+id/nav_square"
        android:name="com.demo.jetpack.navigation.fragment.NavSquareFragment"
        android:label="fragment_square"
        tools:layout="@layout/fragment_nav_square" />
    <fragment
        android:id="@+id/nav_pub"
        android:name="com.demo.jetpack.navigation.fragment.NavPublicFragment"
        android:label="fragment_public"
        tools:layout="@layout/fragment_nav_public" />
    <fragment
        android:id="@+id/nav_ask"
        android:name="com.demo.jetpack.navigation.fragment.NavAskFragment"
        android:label="fragment_ask"
        tools:layout="@layout/fragment_nav_ask" />

</navigation>